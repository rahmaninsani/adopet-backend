{
  "name": "adopet-backend",
  "version": "1.0.0",
  "description": "Adopet Backend",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "db-create": "sequelize-cli db:create",
    "table-create": "sequelize-cli db:migrate",
    "table-drop": "sequelize-cli db:migrate:undo:all",
    "row-insert": "sequelize-cli db:seed:all",
    "row-delete": "sequelize-cli db:seed:undo:all",
    "db-reset": "sequelize-cli db:migrate:undo:all && sequelize-cli db:migrate && sequelize-cli db:seed:all"
  },
  "keywords": [],
  "author": "Rahman Insani <rahmaninsani54@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "sequelize": "^6.21.3",
    "sequelize-cli": "^6.4.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.19"
  },
  "engines": {
    "node": "^16.16.0",
    "yarn": "^1.22.19"
  }
}
